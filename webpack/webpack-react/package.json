{
  "name": "webpack-react",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env process.env.NODE_ENV=development && webpack-dev-server --config webpack.config.dev.js",
    "build": "npm-run-all --parallel build:**",
    "build:production": "rimraf dist && cross-env process.env.NODE_ENV=production && webpack --display-used-exports --config webpack.config.prod.js",
    "build-react": "webpack --config webpack.config.react.js",
    "build-react-production": "cross-env process.env.NODE_ENV=production && webpack --config webpack.config.react.prod.js",
    "build-lib": "webpack --config webpack.config.lib.js"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.1",
    "html-webpack-include-assets-plugin": "^1.0.7",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
